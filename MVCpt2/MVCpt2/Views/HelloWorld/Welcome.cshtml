@{
    ViewData["Title"] = "Welcome";
}

<h2>Welcome</h2>
@*currently set up to show a message for each time the page is visited*@
<ul>
    @for (int i = 0; i < (int)ViewData["NumTimes"]; i++)
    {
        <li>@ViewData["Message"]</li>
    }
</ul>